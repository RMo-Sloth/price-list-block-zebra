{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"attributes": {
		"price_records": {
			"default": [],
			"query": {
				"name": {
					"type": "string",
					"default": ""
				},
				"price": {
					"type": "string",
					"default": "0"
				},
				"index": {
					"type": "number",
					"default": null
				}
			},
			"type": "array"
		},
		"price_record_latest_index": {
			"type": "number",
			"default": 0
		},
		"settings": {
			"type": "object",
			"default": {
				"delete": false,
				"add": true,
				"edit_description": true,
				"edit_price": true,
				"order_items": false
			}
		}
	},
	"name": "price-list-block-zebra/price-list-block-zebra",
	"version": "0.1.0",
	"title": "Price List",
	"category": "widgets",
	"icon": "editor-justify",
	"description": "Create a price list.",
	"supports": {
		"html": false
	},
	"textdomain": "price-list-block-zebra",
	"editorScript": "file:./build/index.js",
	"style": "file:./build/save.css"
}