{
	"name": "price-list-block-zebra",
	"version": "0.1.0",
	"description": "A wordpress block for creating price lists. You can use it to create a menu for a restaurant, prices for a barber shop or anything like it. All editing is done inside the block editor. No confusing menus, no settings. Just easy to use!",
	"author": "robmo",
	"license": "GPL-2.0-or-later ( modify later )",
	"keywords": [],
	"homepage": "https://waardwebsites.nl",
	"repository": {
		"type": "git",
		"url": "https://github.com/RMo-Sloth/price-list-block-zebra.git"
	},
	"bugs": {
		"email": "rob@waardwebsites.nl"
	},
	"main": "build/index.js",
	"dependencies": {
		"@wordpress/block-editor": "^11",
		"@wordpress/blocks": "^12",
		"@wordpress/components": "^23",
		"@wordpress/element": "^5",
		"@wordpress/i18n": "^4"
	},
	"devDependencies": {
		"@wordpress/scripts": "^25.0.0"
	},
	"scripts": {
		"build": "wp-scripts build",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses --prod --gpl2",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint": "wp-scripts lint-pkg-json || wp-scripts lint-style || wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start",
		"stop": "wp-env stop",
		"plugin-zip": "wp-scripts plugin-zip"
	}
